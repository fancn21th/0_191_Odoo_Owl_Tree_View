      <script type="text/javascript" src="/tree_view/static/src/components/tree_item/TreeItem.js"></script>

      <script type="text/javascript" src="/tree_view/static/src/tree_view/tree_view.js"></script>
      <script type="text/javascript" src="/tree_view/static/src/tree_view/tree_view_model.js"></script>
      <script type="text/javascript" src="/tree_view/static/src/tree_view/tree_view_controller.js"></script>
      <script type="text/javascript" src="/tree_view/static/src/tree_view/tree_view_renderer.js"></script>
    </xpath>
    <xpath expr="link[last()]" position="after">
      <link rel="stylesheet" type="text/scss" href="/tree_view/static/src/components/tree_item/tree_item.scss"/>
      <link rel="stylesheet" type="text/scss" href="/tree_view/static/src/tree_view/tree_view.scss"/>
